fn range(a: int, b: int) co int {
	% we can declare local functions
	% note: not a closure, doesn't capture any vars
	fn range(a: int, b: int) int {
		while(a < b) {
			yield a;
			a += 1;
		}
	}
	return create(range, a, b);
}

fn main() void {
	% coroutine objects as iterators
	for (x: range(0, 10)) {
		println(x);
	}
}


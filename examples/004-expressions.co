% stdout: "30\n-10\n-20\n200\n2\n2\n16\n6\n1\n0\n1\nfoobar\n"

fn main() void {
	let a: int = 10;
	let b: int = 20;

	println(a + b);
	println(a - b);
	println(-b);
	println(a * b);
	println(b / a);
	println(b mod 3);
	println(6 * 3 - 2);
	println(6 * (3 - 2));

	% no bool type, 1 is true and 0 is false
	println(1 || 0); 
	println(1 && 0);
	println(!0);

	% string concat
	println("foo" + "bar");
}


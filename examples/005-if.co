% stdout: "ok\nx is smaller\n"

fn main() void {
	let x: int = 10;
	let y: int = 20;

	% {} around the branches is optional
	% unless the branch is an if/while/... (can itself have a body)
	if (x < 30) println("ok");

	if (x < y) {
		println("x is smaller");
	} else {
		println("x is larger");
	}
}

